{
  "cli": {
    "version": ">= 16.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "DATABASE_URL": "${DATABASE_URL}",
        "JWT_SECRET": "${JWT_SECRET}"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "DATABASE_URL": "${DATABASE_URL}",
        "JWT_SECRET": "${JWT_SECRET}"
      }
    }
  },
  "submit": {
    "production": {}
  }
}

